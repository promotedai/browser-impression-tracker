{
  "name": "browser-impression-tracker",
  "version": "0.0.0",
  "description": "Tracks impressions in the browser for logging to Promoted.ai",
  "scripts": {
    "prettier": "prettier '**/*.{js,ts}' --ignore-path ./.prettierignore",
    "types": "tsc --noEmit",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "rimraf dist",
    "build:lib": "rollup -c",
    "build:watch": "rollup -c -w",
    "updateLink": "npm run build:clean && npm run build && cd dist && npm link",
    "lint": "eslint './src/**/*.ts'",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "size": "size-limit",
    "finish": "jest --coverage && npm run lint && npm run types",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/promotedai/browser-impression-tracker"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "library",
    "impression",
    "tracking",
    "promoted"
  ],
  "author": {
    "name": "Dan Hill",
    "email": "dhill@promoted.ai",
    "url": "https://promoted.ai"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/promotedia/browser-impression-tracker/issues"
  },
  "homepage": "https://github.com/promotedai/browser-impression-tracker#readme",
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.0",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@types/jest": "^29.1.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "intersection-observer": "^0.12.0",
    "jest": "^29.1.1",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^12.5.0",
    "prettier": "^2.6.2",
    "rollup": "^2.75.4",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "semantic-release": "^22.0.5",
    "size-limit": "^7.0.8",
    "ts-jest": "^29.1.1",
    "typescript": "^4.7.2",
    "utility-types": "^3.10.0"
  },
  "dependencies": {
    "intersection-observer": "^0.12.2"
  }
}
